<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel="tests" name="Suite">
    
    <listeners>
        <listener class-name="rahulshettyacademy.TestComponents.Listeners"></listener>
